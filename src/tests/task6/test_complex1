prog
var x; // declare variable x
var i;
var a;
var b;
var q;
x := 1;
i := 0;
goto dowork;

dowork:
if (x > 4) goto lexit;
x := x * 3;
goto dowork;

lunknown:
if (i < x) goto lexit;
i := i - 2;
a := 2;
q := 3;
b := 4 / 2;
goto lunknown;

lbig:
if (i < x) goto lexit;
q := 2;
b := 2;
output q;
goto lunknown;

lexit:
output x;
endprog
